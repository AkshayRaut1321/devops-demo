{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "MongoDbSettings": {
      "ConnectionString": "mongodb:27017/?replicaSet=rs0",
      "Database": "productdb",
      "Collection": "products"
    },
    "Elasticsearch": {
      "Url": "http://elasticsearch:9200",
      "IndexName": "products"
    },
    "Worker": {
      "RetryDelaySeconds": 5,
      "EnableChangeStream": true,
      "EnableFullResyncOnStart": false,
      "BatchSize": 100,
      "CheckpointCollection": "search_sync_checkpoints",
      "DeadLetterCollection": "search_sync_deadletters",
      "FullReindexOnStartup": true,
      "FullReindexIntervalMinutes": 1440,
      "ChangeStreamEnabled": true,
      "ChangeStreamRetrySeconds": 10
    },
    "Serilog": {
      "MinimumLevel": "Information",
      "WriteTo": [
        {
          "Name": "Console"
        }
      ]
    }
  }